                "tags": ["SEARCH BREED"],
                "summary": "Get one dog breed request.",
                "description": "Here you can create get one dog breed request.",
                "parameters": [
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "All dogs",
                        "style": "form",
                        "explode": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32",
                            "default": 16
