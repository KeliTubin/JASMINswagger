                    {
                        "name": "limit",
                        "in": "query",
                        "description": "All dogs",
                        "style": "form",
                        "explode": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32",
                            "default": 16
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Dog breed delete request SUCCESSED!"
                    },
                    "500": {
                        "description": "Dog breed delete request FAILED!"
                    }
                }
            }                
        }
